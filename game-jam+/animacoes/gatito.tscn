script/source = "extends Node2D
@onready var body: Sprite2D = $body

var verticaltween

func land():
	if verticaltween: verticaltween.kill()
	scale.y = 0.6
	verticaltween = create_tween().set_ease(Tween.EASE_OUT).set_trans(Tween.TRANS_EXPO)
	verticaltween.tween_property(self, \"scale:y\", 1, 0.4)

func incline(x):
	skew = x/3000.0
"

